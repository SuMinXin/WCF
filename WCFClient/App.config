<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.serviceModel>
		<bindings>
			<wsHttpBinding>
				<binding name="WSHttpBinding_IService">
					<security mode="Message">
						<message clientCredentialType="UserName" />
					</security>
				</binding>
				<binding name="SecuredByTransportEndpoint">
					<security>
						<message clientCredentialType="UserName" />
					</security>
				</binding>
			</wsHttpBinding>
		</bindings>
		<client>
			<!--
			<endpoint address="http://machineName/Service" binding="wsHttpBinding" 
			bindingConfiguration="WSHttpBinding_IService" contract="Service" 
			name="WSHttpBinding_IService">
			</endpoint>
			-->
			<endpoint address="http://localhost:60366/Service.svc" binding="wsHttpBinding"
			 bindingConfiguration="SecuredByTransportEndpoint" contract="ServiceReference.IService"
			 name="SecuredByTransportEndpoint">
				<identity>
					<certificate encodedValue="" />
				</identity>
			</endpoint>
		</client>
	</system.serviceModel>
</configuration>